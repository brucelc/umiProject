{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "noUnusedLocals": true,
    "baseUrl": ".",
    // "strict": true,
    "paths": {
      "@/*": ["src/*"],
      "utils/*": ["src/utils/*"],
      "components/*": ["src/components/*"],
      "pages/*": ["src/pages/*"],
      "models/*": ["src/models/*"],
      "constants/*": ["src/constants/*"],
    },
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
  }
}